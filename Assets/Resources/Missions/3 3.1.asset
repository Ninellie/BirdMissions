    \u0442\u0430\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0435 \u0443\u0433\u043E\u0434\u044C\u044F
    \u0441\u0435\u0431\u0435.  \u0422\u043E\u043B\u044C\u043A\u043E \u043C\u0435\u0441\u0442\u043D\u044B\u0435
    \u043E\u0431\u0438\u0442\u0430\u0442\u0435\u043B\u0438 \u0437\u043D\u0430\u044E\u0442
    \u0432\u0441\u0435 \u0442\u0430\u0439\u043D\u044B \u0427\u0430\u0449\u0438. \u0415\u0441\u043B\u0438
