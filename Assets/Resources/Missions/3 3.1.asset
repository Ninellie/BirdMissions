  _mapCoordinates: {x: -1.6, y: -2}
  _preMissionText: "\u0412 \u0427\u0430\u0449\u0435 \u0442\u0435\u043C\u043D\u043E
    \u0438 \u043C\u0448\u0438\u0441\u0442\u043E. \u0413\u0435\u0440\u043E\u0439 \u043E\u0442\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u0442\u0441\u044F
    \u0432\u044B\u044F\u0441\u043D\u044F\u0442\u044C, \u043F\u0440\u043E\u043F\u0430\u0434\u0430\u044E\u0442
    \u043B\u0438 \u043F\u0442\u0438\u0446\u044B \u0442\u0430\u043C. \u041D\u043E
    \u043E\u0431\u043D\u0430\u0440\u0443\u0436\u0438\u0432\u0430\u0435\u0442, \u0447\u0442\u043E
